# ADB Query Viewer Configuration
# Copy this file to .env and update with your specific values

# Package name for the Android app
PACKAGE_NAME=com.winitsoftware.emiratessnacks

# Database name on the device
DB_NAME=master_data.db

# Device serial number (use 'adb devices' to find it)
DEVICE_SERIAL=your_device_serial_here

# Python tools path (adjust to your project structure)
PYTHON_TOOLS_PATH=/path/to/your/python_tools

# Flask server configuration
FLASK_HOST=0.0.0.0
FLASK_PORT=5001
FLASK_DEBUG=True

# Cache configuration
USE_CACHE=True
FORCE_LOCAL=False

# Test script configuration
# Comma-separated list of tables for show_table_info.sh
TABLE_LIST=table1,table2,table3

# Tables to use in test scripts
TEST_TABLE=table1
TEST_TABLE_2=table2
